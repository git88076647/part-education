CREATE TABLE `kelun_hnym`.`table1`  (
  `id` bigint NOT NULL COMMENT '主键',
  `create_by` bigint NOT NULL DEFAULT 0 COMMENT '创建人',
  `create_time` datetime NOT NULL COMMENT '创建时间',
  `update_by` bigint NULL COMMENT '修改人',
  `update_time` datetime NULL COMMENT '修改时间',
  `version` bigint not NULL DEFAULT 1 COMMENT '版本',
  PRIMARY KEY (`id`)
);

ALTER TABLE `kelun_hnym`.`table1` 
ADD COLUMN `create_by` bigint not NULL DEFAULT 0 COMMENT '创建人' AFTER `id`,
ADD COLUMN `create_time` datetime not NULL DEFAULT now() COMMENT '创建时间' AFTER `create_by`,
ADD COLUMN `update_by` bigint NULL COMMENT '修改人' AFTER `create_time`,
ADD COLUMN `update_time` datetime NULL COMMENT '修改时间' AFTER `update_by`,
ADD COLUMN `version` bigint not NULL DEFAULT 1 COMMENT '版本' AFTER `update_time`;